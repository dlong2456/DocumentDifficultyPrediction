{
  "manifest_version": 2,
  "name": "Document Difficulty Prediction",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "webRequest",
    "activeTab",
    "tabs",
    "management",
    "*://*.google.com/"
   ], 
  "content_scripts": [
  {
    "matches": [
        "<all_urls>"
    ],
    "js": ["content.js"]
  }
  ]
}