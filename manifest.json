{
  "manifest_version": 2,
  "name": "Document Difficulty Prediction",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
   "activeTab",
   "https://docs.google.com/",
   "webRequest"
   ], 
  "content_scripts": [
    {
      "matches": [
            "http://*/*",
            "https://*/*"
      ],
      "css": ["thirdParty/jquery-ui-1.11.4.custom/jquery-ui.css"],
      "js": ["thirdParty/jquery-1.11.3.js", "thirdParty/jquery-ui-1.11.4.custom/jquery-ui.js", "content.js"]
    }
  ]
}