{
  "manifest_version": 2,
  "name": "Document Difficulty Prediction",
  "version": "0.1",
  "background": {
    "scripts": ["client/background.js"],
    "persistent": true
  },
  "permissions": [
    "webRequest",
    "activeTab",
    "tabs",
    "management",
    "*://*.google.com/"
   ], 
  "content_scripts": [
  {
    "matches": [
        "<all_urls>"
    ],
    "js": ["client/thirdParty/jquery-2.1.4.min.js", "client/content.js"], 
    "html": ["client/dialog.html"], 
    "css": ["client/custom.css"]
  }
  ]
}